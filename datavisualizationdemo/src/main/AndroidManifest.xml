<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.supermap.datavisualizationdemo">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".appconfig.MyApplication">
        <activity
            android:name="com.supermap.datavisualizationdemo.appconfig.StartupActivity"
            android:label="@string/app_name"
            android:icon="@drawable/icon_mchart">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.supermap.datavisualizationdemo.DataVisualization.MainActivity"
            android:label="@string/app_name"
            android:icon="@drawable/icon_mchart"
            android:configChanges="orientation|keyboardHidden" >
        </activity>
    </application>
</manifest>
